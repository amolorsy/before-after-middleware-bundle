{
    "name": "gyu-kang/before-after-middleware-bundle",
    "description": "A symfony bundle for implementing a middleware run before or after a request",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Gyu Kang",
            "email": "gyu@gyukang.me"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "BeforeAfterMiddlewareBundle\\": "src/BeforeAfterMiddlewareBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeforeAfterMiddlewareBundle\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.1.3",
        "symfony/symfony": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
